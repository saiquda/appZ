@{
}
@model IEnumerable<Subject>
<table class="table" border="0" cellpadding="0" cellspacing="2" id="subjects-table">
    @foreach (var item in Model)
    {
        <tr>
            <td>
                <button class="subject"  data-SubjectId="@item.Id">@item.Name</button>
            </td>
        </tr>
    }
</table>

